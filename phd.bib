@article{artis_impact_2021,
	title = {Impact of the calibration of the halo mass function on galaxy cluster number count cosmology},
	volume = {649},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/2021A&A...649A..47A/abstract},
	doi = {10.1051/0004-6361/202140293},
	abstract = {The halo mass function ({HMF}) is a critical element in cosmological analyses of galaxy cluster catalogs. We quantify the impact of uncertainties in {HMF} parameters on cosmological constraints from cluster catalogs similar to those from Planck, those expected from the Euclid, Roman, and Rubin surveys, and from a hypothetical larger future survey. We analyze simulated catalogs in each case, gradually loosening priors on {HMF} parameters to evaluate the degradation in cosmological constraints. While current uncertainties on {HMF} parameters do not substantially impact Planck-like surveys, we find that they can significantly degrade the cosmological constraints for a Euclid-like survey. Consequently, the current precision on the {HMF} is not sufficient for Euclid, Roman, Rubin or possible larger surveys. Future experiments will have to properly account for uncertainties in {HMF} parameters, and it will be necessary to improve the precision of {HMF} fits to avoid weakening constraints on cosmological parameters.},
	pages = {A47},
	journaltitle = {Astronomy and Astrophysics},
	author = {Artis, Emmanuel and Melin, Jean-Baptiste and Bartlett, James G. and Murray, Calum},
	urldate = {2021-07-20},
	date = {2021-05},
	langid = {english},
	file = {Snapshot:/Users/keruzore/Zotero/storage/JHMXEXTJ/abstract.html:text/html;Full Text PDF:/Users/keruzore/Zotero/storage/2GXW6QX2/Artis et al. - 2021 - Impact of the calibration of the halo mass functio.pdf:application/pdf},
}

@article{arnaud_universal_2010,
	title = {The universal galaxy cluster pressure profile from a representative sample of nearby systems ({REXCESS}) and the {YSZ} – M500 relation},
	volume = {517},
	rights = {© {ESO}, 2010},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2010/09/aa13416-09/aa13416-09.html},
	doi = {10.1051/0004-6361/200913416},
	abstract = {We investigate the regularity of cluster pressure profiles with {REXCESS}, a representative sample of 33 local ({\textless}i{\textgreater}z{\textless}i/{\textgreater} {\textless} 0.2) clusters drawn from the {REFLEX} catalogue and observed with {\textless}i{\textgreater}{XMM}-Newton{\textless}i/{\textgreater}. The sample spans a mass range of 10{\textless}sup{\textgreater}14{\textless}sup/{\textgreater} {\textless} {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} {\textless} 10{\textless}sup{\textgreater}15{\textless}sup/{\textgreater} , where {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} is the mass corresponding to a density contrast of 500. We derive an average profile from observations scaled by mass and redshift according to the standard self-similar model, and find that the dispersion about the mean is remarkably low, at less than 30 per cent beyond 0.2 {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}, but increases towards the center. Deviations about the mean are related to both the mass and the thermo-dynamical state of the cluster. Morphologically disturbed systems have systematically shallower profiles while cooling core systems are more concentrated. The scaled profiles exhibit a residual mass dependence with a slope of {\textasciitilde}0.12, consistent with that expected from the empirically-derived slope of the {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} – {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}X{\textless}sub/{\textgreater} relation; however, the departure from standard scaling decreases with radius and is consistent with zero at {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}. The scatter in the core and departure from self-similar mass scaling is smaller compared to that of the entropy profiles, showing that the pressure is the quantity least affected by dynamical history and non-gravitational physics. Comparison with scaled data from several state of the art numerical simulations shows good agreement outside the core. Combining the observational data in the radial range [0.03–1] {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} with simulation data in the radial range [1–4] {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}, we derive a robust measure of the universal pressure profile, that, in an analytical form, defines the physical pressure profile of clusters as a function of mass and redshift up to the cluster “boundary”. Using this profile and direct spherical integration of the observed pressure profiles, we estimate the integrated Compton parameter {\textless}i{\textgreater}Y{\textless}i/{\textgreater} and investigate its scaling with {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} and {\textless}i{\textgreater}L{\textless}i/{\textgreater}{\textless}sub{\textgreater}X{\textless}sub/{\textgreater}, the soft band X-ray luminosity. We consider both the spherically integrated quantity, {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}sph{\textless}sub/{\textgreater}({\textless}i{\textgreater}R{\textless}i/{\textgreater}), proportional to the gas thermal energy, and the cylindrically integrated quantity, {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}cyl{\textless}sub/{\textgreater}({\textless}i{\textgreater}R{\textless}i/{\textgreater})={\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}{SZ}{\textless}sub/{\textgreater} {\textless}i{\textgreater}D{\textless}i/{\textgreater}{\textless}sub{\textgreater}A{\textless}sub/{\textgreater}{\textless}sup{\textgreater}2{\textless}sup/{\textgreater}, which is directly related to the Sunyaev-Zel'dovich ({SZ}) effect signal. From the low scatter of the observed {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}sph{\textless}sub/{\textgreater}({\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}) – {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}X{\textless}sub/{\textgreater} relation we show that variations in pressure profile shape do not introduce extra scatter into the {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}sph{\textless}sub/{\textgreater}({\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}) – {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} relation as compared to that from the {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}X{\textless}sub/{\textgreater} – {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} relation. The {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}sph{\textless}sub/{\textgreater}({\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}) – {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} and {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}sph{\textless}sub/{\textgreater}({\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}) – {\textless}i{\textgreater}L{\textless}i/{\textgreater}{\textless}sub{\textgreater}X{\textless}sub/{\textgreater} relations derived from the data are in excellent agreement with those expected from the universal profile. This profile is used to derive the expected {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}{SZ}{\textless}sub/{\textgreater} – {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} and {\textless}i{\textgreater}Y{\textless}i/{\textgreater}{\textless}sub{\textgreater}{SZ}{\textless}sub/{\textgreater} – {\textless}i{\textgreater}L{\textless}i/{\textgreater}{\textless}sub{\textgreater}X{\textless}sub/{\textgreater} relations for any aperture.},
	pages = {A92},
	journaltitle = {Astronomy \& Astrophysics},
	shortjournal = {A\&A},
	author = {Arnaud, M. and Pratt, G. W. and Piffaretti, R. and Böhringer, H. and Croston, J. H. and Pointecouteau, E.},
	urldate = {2019-02-21},
	date = {2010-07-01},
	langid = {english},
	file = {Snapshot:/Users/keruzore/Zotero/storage/GMSF4B6N/aa13416-09.html:text/html;Full Text PDF:/Users/keruzore/Zotero/storage/GYVGTWP6/Arnaud et al. - 2010 - The universal galaxy cluster pressure profile from.pdf:application/pdf},
}

@article{andreon_measurement_2013,
