@article{prada_halo_2012,
  title = {Halo Concentrations in the Standard {{$\Lambda$}} Cold Dark Matter Cosmology},
  author = {Prada, Francisco and Klypin, Anatoly A. and Cuesta, Antonio J. and Betancort-Rijo, Juan E. and Primack, Joel},
  date = {2012-07-11},
  journaltitle = {MNRAS},
