@article{zubeldia_cosmological_2019,
  title = {Cosmological Constraints from {{Planck}} Galaxy Clusters with {{CMB}} Lensing Mass Bias Calibration},
  author = {Zubeldia, \'I\~nigo and Challinor, Anthony},
  date = {2019-10-11},
