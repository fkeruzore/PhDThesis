@article{kozmanyan_deriving_2019,
	title = {Deriving the Hubble constant using Planck and {XMM}-Newton observations of galaxy clusters},
	volume = {621},
	issn = {0004-6361},
	url = {http://adsabs.harvard.edu/abs/2019A%26A...621A..34K},
	doi = {10.1051/0004-6361/201833879},
	abstract = {The possibility of determining the value of the Hubble constant using 
observations of galaxy clusters in X-ray and microwave wavelengths
through the Sunyaev Zel'dovich ({SZ}) effect has long been known. Previous
measurements have been plagued by relatively large errors in the
observational data and severe biases induced, for example, by cluster
triaxiality and clumpiness. The advent of Planck allows us to map the
Compton parameter y, that is, the amplitude of the {SZ} effect, with
unprecedented accuracy at intermediate cluster-centric radii, which in
turn allows performing a detailed spatially resolved comparison with
X-ray measurements. Given such higher quality observational data, we
developed a Bayesian approach that combines informed priors on the
physics of the intracluster medium obtained from hydrodynamical
simulations of massive clusters with measurement uncertainties. We
applied our method to a sample of 61 galaxy clusters with redshifts up
to z {\textless} 0.5 observed with Planck and {XMM}-Newton and find H0
= 67 ± 3 km s-1 Mpc-1.},
	pages = {A34},
	journaltitle = {Astronomy and Astrophysics},
	shortjournal = {Astronomy and Astrophysics},
	author = {Kozmanyan, Arpine and Bourdin, Hervé and Mazzotta, Pasquale and Rasia, Elena and Sereno, Mauro},
	urldate = {2021-04-01},
	date = {2019-01-01},
	keywords = {cosmological parameters, galaxies: clusters: general, galaxies: clusters: intracluster medium, X-rays: galaxies: clusters, distance scale},
	file = {Full Text PDF:/Users/keruzore/Zotero/storage/JY6HFXMI/Kozmanyan et al. - 2019 - Deriving the Hubble constant using Planck and XMM-.pdf:application/pdf},
}

