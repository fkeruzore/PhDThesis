@article{ade_planck_2013,
	title = {Planck intermediate results - X. Physics of the hot gas in the Coma cluster},
	volume = {554},
	rights = {© {ESO}, 2013},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2013/06/aa20247-12/aa20247-12.html},
	doi = {10.1051/0004-6361/201220247},
	abstract = {We present an analysis of {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} satellite data on the Coma cluster observed via the Sunyaev-Zeldovich effect. Thanks to its great sensitivity, {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} is able, for the first time, to detect {SZ} emission up to {\textless}i{\textgreater}r{\textless}i/{\textgreater} ≈ 3 × {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}. We test previously proposed spherically symmetric models for the pressure distribution in clusters against the azimuthally averaged data. In particular, we find that the Arnaud et al. (2010, A\&A, 517, A92) “universal” pressure profile does not fit Coma, and that their pressure profile for merging systems provides a reasonable fit to the data only at {\textless}i{\textgreater}r{\textless}i/{\textgreater} {\textless} {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}; by {\textless}i{\textgreater}r{\textless}i/{\textgreater} = 2 × {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} it underestimates the observed {\textless}i{\textgreater}y{\textless}i/{\textgreater} profile by a factor of ≃2. This may indicate that at these larger radii either: i) the cluster {SZ} emission is contaminated by unresolved {SZ} sources along the line of sight; or ii) the pressure profile of Coma is higher at {\textless}i{\textgreater}r{\textless}i/{\textgreater} {\textgreater} {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater} than the mean pressure profile predicted by the simulations used to constrain the models. The {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} image shows significant local steepening of the {\textless}i{\textgreater}y{\textless}i/{\textgreater} profile in two regions about half a degree to the west and to the south-east of the cluster centre. These features are consistent with the presence of shock fronts at these radii, and indeed the western feature was previously noticed in the {ROSAT} {PSPC} mosaic as well as in the radio. Using {\textless}i{\textgreater}Planck{\textless}i/{\textgreater}{\textless}i{\textgreater}y{\textless}i/{\textgreater} profiles extracted from corresponding sectors we find pressure jumps of 4.9{\textless}sub{\textgreater}-0.2{\textless}sub/{\textgreater}{\textless}sup{\textgreater}+0.4{\textless}sup/{\textgreater} and 5.0{\textless}sub{\textgreater}-0.1{\textless}sub/{\textgreater}{\textless}sup{\textgreater}+1.3{\textless}sup/{\textgreater} in the west and south-east, respectively. Assuming Rankine-Hugoniot pressure jump conditions, we deduce that the shock waves should propagate with Mach number {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}w{\textless}sub/{\textgreater} = 2.03{\textless}sub{\textgreater}-0.04{\textless}sub/{\textgreater}{\textless}sup{\textgreater}+0.09{\textless}sup/{\textgreater} and {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}se{\textless}sub/{\textgreater} = 2.05{\textless}sub{\textgreater}-0.02{\textless}sub/{\textgreater}{\textless}sup{\textgreater}+0.25{\textless}sup/{\textgreater} in the west and south-east, respectively. Finally, we find that the {\textless}i{\textgreater}y{\textless}i/{\textgreater} and radio-synchrotron signals are quasi-linearly correlated on Mpc scales, with small intrinsic scatter. This implies either that the energy density of cosmic-ray electrons is relatively constant throughout the cluster, or that the magnetic fields fall off much more slowly with radius than previously thought.},
	pages = {A140},
	journaltitle = {Astronomy \& Astrophysics},
