	journaltitle = {Astronomy \& Astrophysics},
	shortjournal = {A\&A},
	author = {Adam, R. and Adane, A. and Ade, P. a. R. and André, P. and Andrianasolo, A. and Aussel, H. and Beelen, A. and Benoît, A. and Bideaud, A. and Billot, N. and Bourrion, O. and Bracco, A. and Calvo, M. and Catalano, A. and Coiffard, G. and Comis, B. and Petris, M. De and Désert, F.-X. and Doyle, S. and Driessen, E. F. C. and Evans, R. and Goupy, J. and Kramer, C. and Lagache, G. and Leclercq, S. and Leggeri, J.-P. and Lestrade, J.-F. and Macías-Pérez, J. F. and Mauskopf, P. and Mayet, F. and Maury, A. and Monfardini, A. and Navarro, S. and Pascale, E. and Perotto, L. and Pisano, G. and Ponthieu, N. and Revéret, V. and Rigby, A. and Ritacco, A. and Romero, C. and Roussel, H. and Ruppin, F. and Schuster, K. and Sievers, A. and Triqueneaux, S. and Tucker, C. and Zylka, R.},
