@article{keruzore_exploiting_2020,
  title = {Exploiting {{NIKA2}}/{{XMM}}-{{Newton}} Imaging Synergy for Intermediate-Mass High-z Galaxy Clusters within the {{NIKA2 SZ}} Large Program - {{Observations}} of {{ACT}}-{{CL J0215}}.4+0030 at z {$\sim$} 0.9},
  author = {K\'eruzor\'e, F. and Mayet, F. and Pratt, G. W. and Adam, R. and Ade, P. and Andr\'e, P. and Andrianasolo, A. and Arnaud, M. and Aussel, H. and Bartalucci, I. and Beelen, A. and Beno\^it, A. and Berta, S. and Bourrion, O. and Calvo, M. and Catalano, A. and Petris, M. De and D\'esert, F.-X. and Doyle, S. and Driessen, E. F. C. and Gomez, A. and Goupy, J. and Kramer, C. and Ladjelate, B. and Lagache, G. and Leclercq, S. and Lestrade, J.-F. and Mac\'ias-P\'erez, J. F. and Mauskopf, P. and Monfardini, A. and Perotto, L. and Pisano, G. and Pointecouteau, E. and Ponthieu, N. and Rev\'eret, V. and Ritacco, A. and Romero, C. and Roussel, H. and Ruppin, F. and Schuster, K. and Shu, S. and Sievers, A. and Tucker, C.},
