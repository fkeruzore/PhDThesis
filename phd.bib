  urldate = {2019-02-21},
  abstract = {The determination of the thermodynamic properties of clusters of galaxies at intermediate and high redshift can bring new insights into the formation of large-scale structures. It is essential for a robust calibration of the mass-observable scaling relations and their scatter, which are key ingredients for precise cosmology using cluster statistics. Here we illustrate an application of high resolution ({$<$}20 arcsec) thermal Sunyaev-Zel'dovich (tSZ) observations by probing the intracluster medium (ICM) of the \emph{Planck{$<$}i/{$>$}-discovered galaxy cluster PSZ1 G045.85+57.71 at redshift \emph{z{$<$}i/{$>$} = 0.61, using tSZ data obtained with the NIKA camera, which is a dual-band (150 and 260 GHz) instrument operated at the IRAM 30-m telescope. We deproject jointly NIKA and \emph{Planck{$<$}i/{$>$} data to extract the electronic pressure distribution from the cluster core (\emph{R{$<$}i/{$>$} \textasciitilde{} 0.02 \emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}) to its outskirts (\emph{R{$<$}i/{$>$} \textasciitilde{} 3 \emph{R{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$}) non-parametrically for the first time at intermediate redshift. The constraints on the resulting pressure profile allow us to reduce the relative uncertainty on the integrated Compton parameter by a factor of two compared to the \emph{Planck{$<$}i/{$>$} value. Combining the tSZ data and the deprojected electronic density profile from \emph{XMM-Newton{$<$}i/{$>$} allows us to undertake a hydrostatic mass analysis, for which we study the impact of a spherical model assumption on the total mass estimate. We also investigate the radial temperature and entropy distributions. These data indicate that PSZ1 G045.85+57.71 is a massive (\emph{M{$<$}i/{$>$}\textsubscript{500{$<$}sub/{$>$} \textasciitilde{} 5.5 \texttimes{} 10\textsuperscript{14{$<$}sup/{$>$}\emph{M{$<$}i/{$>$}\textsubscript{{$\odot<$}sub/{$>$}) cool-core cluster. This work is part of a pilot study aiming at optimizing the treatment of the NIKA2 tSZ large program dedicated to the follow-up of SZ-discovered clusters at intermediate and high redshifts. This study illustrates the potential of NIKA2 to put constraints on thethermodynamic properties and tSZ-scaling relations of these clusters, and demonstrates the excellent synergy between tSZ and X-ray observations of similar angular resolution.}}}}}}}}}}}}}}}}},
  file = {/Users/keruzore/Zotero/storage/C5CJHEUW/Ruppin et al. - 2017 - Non-parametric deprojection of NIKA SZ observation.pdf;/Users/keruzore/Zotero/storage/9SJ7XUBL/aa29405-16.html},
  langid = {english}
}

@article{ruppin_stability_2020,
  title = {Stability of {{Cool Cores During Galaxy Cluster Growth}}: {{A Joint Chandra}}/{{SPT Analysis}} of 67 {{Galaxy Clusters Along}} a {{Common Evolutionary Track Spanning}} 9 {{Gyr}}},
  shorttitle = {Stability of {{Cool Cores During Galaxy Cluster Growth}}},
