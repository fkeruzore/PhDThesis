	urldate = {2019-02-21},
	date = {2013},
	langid = {english},
	keywords = {astrophysics, Bayesian statistics, errors-in-variables, measurement errors, regression, scaling relations},
	file = {Full Text PDF:/Users/keruzore/Zotero/storage/YT3UJ89E/Andreon and Hurn - 2013 - Measurement errors and scaling relations in astrop.pdf:application/pdf;Snapshot:/Users/keruzore/Zotero/storage/MLRQBSS5/sam.html:text/html},
}

