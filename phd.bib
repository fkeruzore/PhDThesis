@article{planck_collaboration_planck_2013,
	title = {Planck intermediate results - V. Pressure profiles of galaxy clusters from the Sunyaev-Zeldovich effect},
	volume = {550},
	rights = {© {ESO}, 2013},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2013/02/aa20040-12/aa20040-12.html},
	doi = {10.1051/0004-6361/201220040},
	abstract = {Taking advantage of the all-sky coverage and broadfrequency range of the {\textless}i{\textgreater}Planck {\textless}i/{\textgreater}satellite, we study the Sunyaev-Zeldovich ({SZ}) and pressure profiles of 62 nearby massive clusters detected at high significance in the 14-month nominal survey. Careful reconstruction of the {SZ} signal indicates that most clusters are individually detected at least out to {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}. By stacking the radial profiles, we have statistically detected the radial {SZ} signal out to 3 × {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}, i.e., at a density contrast of about 50–100, though the dispersion about the mean profile dominates the statistical errors across the whole radial range. Our measurement is fully consistent with previous {\textless}i{\textgreater}Planck {\textless}i/{\textgreater}results on integrated {SZ} fluxes, further strengthening the agreement between {SZ} and X-ray measurements inside {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}. Correcting for the effects of the {\textless}i{\textgreater}Planck {\textless}i/{\textgreater}beam, we have calculated the corresponding pressure profiles. This new constraint from {SZ} measurements is consistent with the X-ray constraints from {\textless}i{\textgreater}{XMM}-Newton{\textless}i/{\textgreater} in the region in which the profiles overlap (i.e., [0.1–1] {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}), and is in fairly good agreement with theoretical predictions within the expected dispersion. At larger radii the average pressure profile is slightly flatter than most predictions from numerical simulations. Combining the {SZ} and X-ray observed profiles into a joint fit to a generalised pressure profile gives best-fit parameters [{\textless}i{\textgreater}P{\textless}i/{\textgreater}{\textless}sub{\textgreater}0{\textless}sub/{\textgreater},{\textless}i{\textgreater}c{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}{\textless}i{\textgreater},γ,α,β{\textless}i/{\textgreater} ] = [6.41,1.81,0.31,1.33,4.13 ] . Using a reasonable hypothesis for the gas temperature in the cluster outskirts we reconstruct from our stacked pressure profile the gas mass fraction profile out to 3 {\textless}i{\textgreater}R{\textless}i/{\textgreater}{\textless}sub{\textgreater}500{\textless}sub/{\textgreater}. Within the temperature driven uncertainties, our {\textless}i{\textgreater}Planck {\textless}i/{\textgreater}constraints are compatible with the cosmic baryon fraction and expected gas fraction in halos.},
	pages = {A131},
	journaltitle = {Astronomy \& Astrophysics},
