	author = {{LSST Dark Energy Science Collaboration}},
	urldate = {2021-03-30},
	date = {2012-11-01},
	keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics, High Energy Physics - Experiment},
	file = {Full Text PDF:/Users/keruzore/Zotero/storage/G7GM68ZV/LSST Dark Energy Science Collaboration - 2012 - Large Synoptic Survey Telescope Dark Energy Scien.pdf:application/pdf},
}

